{
  "name": "use-second",
  "version": "0.0.1",
  "description": "Example react hook for use in remotion to get current second.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "remotion": "4.0.46"
  },
  "devDependencies": {
    "eslint-custom-config": "workspace:*"
  },
  "scripts": {
    "dev": "npm run build",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "watch": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "publishConfig": {
    "access": "public"
  }
}
